$(document).ready(function(){

	let reg = /([@#][^@^#]\S*)/g

	if(location.href.match(reg)){
		setTimeout(function(){
			if(document.querySelector(`a[href="${location.href.match(reg)}"]`)) {
				document.querySelector(`a[href="${location.href.match(reg)}"]`).click()
				console.log('readyyy', location.href.match(reg))
			}
		},1000)

	} 
})



