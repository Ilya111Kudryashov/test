/**
 * Highlight and open on scroll for Titles sidebar
 */


var contentSection = $('.fenix3__main-body-wrapper');

//update navigation on scroll...
$(window).on('scroll', function(){

    updateNavigation();
})
//...and when the page starts
updateNavigation();

$('.fenix3__left-sedebar-link-features a').on('click',  function(event){
    event.cancelBubble = true;
    event.stopPropagation()
    event.preventDefault(); //prevents previous event
    smoothScroll($(this.hash));
    console.log('test')
})




/////FUNCTIONS
function updateNavigation(){

    contentSection.each(function(){

        var sectionName = $(this).attr('id');
        var navigationMatch = $('.fenix3__left-sedebar-item .fenix3__left-sedebar-link-features a#' + sectionName);

        if( $(this).position().top -100 <= $(window).scrollTop())
        {
            $('.fenix3__left-sedebar-link-features').removeClass('fenix3__left-sedebar-link_active-inner');
            $('.fenix3__left-sidebar-submenu').removeClass('fenix3__left-sidebar-submenu_active');
            $('.fenix3__left-sidebar-arrow').removeClass('fenix3__left-sidebar-arrow_active');

            /*navigationMatch.addClass('TEST')*/

            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sedebar-link-features').addClass('fenix3__left-sedebar-link_active-inner')
            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sidebar-submenu').addClass('fenix3__left-sidebar-submenu_active')
            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sidebar-arrow').addClass('fenix3__left-sidebar-arrow_active')
        }
        else {
            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sedebar-link-features').removeClass('fenix3__left-sedebar-link_active-inner')
            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sidebar-submenu').removeClass('fenix3__left-sidebar-submenu_active')
            navigationMatch.parents('.fenix3__left-sedebar-item').find('.fenix3__left-sidebar-arrow').removeClass('fenix3__left-sidebar-arrow_active')
        }
    });
}

function smoothScroll(target){
    $('body,html').animate({
        // scrollTop: target.offset().top - 100
        //scrollTop: ($(window).width() <= 1024) ? target.offset().top - 60 : target.offset().top - 90
    }, 200);

    // if(target[0].id === 'section1'){
    //     $('body,html').animate({
    //         // scrollTop: target.offset().top - 100
    //         scrollTop: ($(window).width() < 1024) ? target.offset().top - 60 : target.offset().top - 500
    //     }, 200);
    // }
}

