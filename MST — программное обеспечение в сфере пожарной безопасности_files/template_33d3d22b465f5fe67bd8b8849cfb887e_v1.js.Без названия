    i.addEventListener('click', function(){
        console.log('test')
        let a = document.querySelector(`.fenix3__main-body-wrapper[data-scroll='${i.dataset.scroll}'] `).scrollIntoView({ behavior: 'smooth', block: 'start'})
        window.scrollTo(0, document.querySelector(`.fenix3__main-body-wrapper[data-scroll='${i.dataset.scroll}'] `).offsetTop - 80)
    })
})
