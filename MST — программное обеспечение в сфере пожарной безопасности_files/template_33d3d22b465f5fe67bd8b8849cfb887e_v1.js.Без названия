$(document).ready(function(){
    

  document.querySelectorAll('.fenix3__left-sedebar-item a').forEach(i=>{
      i.addEventListener('click', function(){

         console.log('test',i.href.split("#")[1])

          document.querySelector(`#${i.href.split("#")[1]}`).scrollIntoView({ behavior: 'smooth', block: 'start'})

          setTimeout(function(){
              window.scrollTo(0, document.querySelector(`#${i.href.split("#")[1]}`).offsetTop - 80)
          },200)

      })
  })
    
})

