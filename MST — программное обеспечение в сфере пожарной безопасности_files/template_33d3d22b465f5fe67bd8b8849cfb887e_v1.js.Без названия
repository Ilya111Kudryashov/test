//FAQ Accordion
$(document).ready(function(){
    $('.faq_item_title').click(function (){
        const parent = $(this).parent();
        if(parent.hasClass('faq_item__active')) {
            parent.removeClass('faq_item__active');
        } else {
            parent.addClass('faq_item__active')
        }
    });
})

