    $('body').on('click', '.fenix3-account-filter__title a', function (){
        $('.fenix3-filter').addClass('fenix3-filter-active')
    });
	
	$('body').on('click', '.fenix3-filter__close svg', function (){
        $('.fenix3-filter').removeClass('fenix3-filter-active')
    });

})


