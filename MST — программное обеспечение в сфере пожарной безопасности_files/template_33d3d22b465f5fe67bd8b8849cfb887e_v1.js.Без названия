$(document).ready(function(){
    if(location.href.match('fenix-server/buy/#server-prolong')){

      $('.tabs__button').each(function(){
       $(this).removeClass('tabs__button--active')
      })
      
      $('button[data-id="server-prolong"').addClass('tabs__button--active')

      $('.tab-content').each(function(){
       $(this).removeClass('tab-content--active')
      })

      $('div[data-content="server-prolong-content"').addClass('tab-content--active')


    }

    if(location.href.match('fenix-server/buy/#server-buy')){

      $('.tabs__button').each(function(){
       $(this).removeClass('tabs__button--active')
      })
      
      $('button[data-id="server-buy"').addClass('tabs__button--active')

      $('.tab-content').each(function(){
       $(this).removeClass('tab-content--active')
      })

      $('div[data-content="server-buy-content"').addClass('tab-content--active')
    }




    if(location.href.match('fenix3/buy/#fenix3-buy') || location.href.match('fireguard4/buy/#fireguard4-buy')){

      $('.tabs__button').each(function(){
       $(this).removeClass('tabs__button--active')
      })
      
      $('button[data-id="fenix3-buy"').addClass('tabs__button--active')

      $('.tab-content').each(function(){
       $(this).removeClass('tab-content--active')
      })

      $('div[data-content="fenix3-buy-content"').addClass('tab-content--active')

    }

    if(location.href.match('fenix3/buy/#fenix3-prolong') || location.href.match('fireguard4/buy/#fireguard4-prolong')){

      $('.tabs__button').each(function(){
       $(this).removeClass('tabs__button--active')
      })
      
      $('button[data-id="fenix3-prolong"').addClass('tabs__button--active')

      $('.tab-content').each(function(){
       $(this).removeClass('tab-content--active')
      })

      $('div[data-content="fenix3-prolong-content"').addClass('tab-content--active')

    }


      $('.tabs__button').each(function(){
        $(this).on('click', function(e){
          e.preventDefault();
	  location.hash = ''
	  location.hash = '#' +	$(this).attr('data-id')
          console.log($(this).attr('id'))
          return false;
        })
      })

})


/**
 * Scroll offset for deployment page
