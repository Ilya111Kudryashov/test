		});
		
		$(this).parent().remove();
	});
});


$(function () {
  $('.fenix3-licence__comparison-btn').on('click', function () {
    if ($(this).is('.active')) {
      $(this).children('.fenix3-licence__comparison-btn-title').children('span').text('Показать');
    } else {
      $(this).children('.fenix3-licence__comparison-btn-title').children('span').text('Скрыть');
    }
    $('.comparison__wrapper').slideToggle();
    $(this).toggleClass('active');
  });
});

$(function () {
  const mobileSelect = $('.fenix3-licence-specifications__table-mobile_select');
  function showMobileTable() {
    const currentType = $('.fenix3-licence-specifications__table-mobile_select button').attr(
      'data-type',
    );
    if (!currentType) {
      $(".fenix3-licence-specifications__table-mobile").children().first().show();
      return;
    }
    $(`table[data-type]`).hide();
    $(`table[data-type=${currentType}]`).show();
  }
  /* Заголовок выпадающего меню */
  mobileSelect
    .find('span')
    .text(mobileSelect.find('li').first().addClass('active').text())
    .parent()
    .attr('data-type', mobileSelect.find('li').first().attr('data-type'));
    showMobileTable();

  /* Открытие/закрытие выпадающего списка */
  mobileSelect.children('button').on('click', function () {
    mobileSelect.toggleClass('active');
    mobileSelect.children('ul').slideToggle();
  });

  mobileSelect.find('li').on('click', function (e) {
    mobileSelect.find('span').text($(this).text());
    mobileSelect.children('ul').slideToggle();
    mobileSelect.toggleClass('active');
    mobileSelect.find('li').removeClass("active");
    $(this).addClass("active");
    $('.fenix3-licence-specifications__table-mobile_select button').attr('data-type', $(this).attr('data-type'));
    showMobileTable();
  });
});

$(function () {
  if ($('.dropdown').length) {
    $('body').on('click', '.dropdown__title', function () {
      $(this).parent().toggleClass('active').find('.dropdown__body').slideToggle();
    });
  }
});

$(function() {
  
  $('.tabs__caption').on('click', 'label', function() {
	console.log($(this).parent().index('.tabs__caption'))
    $(this).parent()
      .addClass('active').addClass('active-input').siblings().removeClass('active').removeClass('active-input')
      .closest('div.tabs__wrapper').find('div.tabs__content').removeClass('active').eq($(this).parent().index('.tabs__caption')).addClass('active');
  });
  
});

jQuery('<div class="quantity-nav"><div class="quantity-button quantity-up"></div><div class="quantity-button quantity-down"></div></div>').insertAfter('.quantity input');
    jQuery('.quantity').each(function() {
      var spinner = jQuery(this),
        input = spinner.find('input[type="number"]'),
        btnUp = spinner.find('.quantity-up'),
        btnDown = spinner.find('.quantity-down'),
        min = input.attr('min'),
        max = input.attr('max');

      btnUp.click(function() {
        var oldValue = parseFloat(input.val());
        if (oldValue >= max) {
          var newVal = oldValue;
        } else {
          var newVal = oldValue + 1;
        }
        spinner.find("input").val(newVal);
        spinner.find("input").trigger("change");
      });

      btnDown.click(function() {
        var oldValue = parseFloat(input.val());
        if (oldValue <= min) {
          var newVal = oldValue;
        } else {
          var newVal = oldValue - 1;
        }
        spinner.find("input").val(newVal);
        spinner.find("input").trigger("change");
      });

    });
	
	
	
const swiper = new Swiper('#comparison__slider', {
  loop: false,
  slidesPerView: 1,
  allowTouchMove: false,
  
  navigation: {
    nextEl: '#sliderNext',
    prevEl: '#sliderPrev',
  },

  
});

swiper.destroy();

const swiperTabConfig ={
  slidesPerView: 1,
  spaceBetween: 10,
  
  navigation: {
      nextEl: '#tabBlockNext',
      prevEl: '#tabBlockPrev',
  },
  breakpoints: {
        350: {
          slidesPerView: "auto",
          spaceBetween: 10,
        },
        768: {
          slidesPerView: "auto",
          spaceBetween: 24,
        },
      },

};


const breakpoint= window.matchMedia('(max-width:1219px)');

const breakpointChecker=function(){
    if(breakpoint.matches===true){
      swiperTab = new Swiper('#tabBlockSlider', swiperTabConfig);
        return;
    }else if(breakpoint.matches===false){

        if(swiperTab !==undefined) swiperTab.destroy(true,true);
        return;
    }
};

breakpoint.addListener(breakpointChecker);
breakpointChecker();
